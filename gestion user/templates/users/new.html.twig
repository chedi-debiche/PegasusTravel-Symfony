{% extends 'baseBack.html.twig' %}

{% block title %}New Evenement{% endblock %}

{% block body %}
    <div style="display: flex;align-items: center;justify-content: center">
    {% form_theme form 'foundation_5_layout.html.twig' %}
    {{ form_start(form) }}
    <h1>Cr√©er un nouveau compte </h1>

    {{ include('users/_form.html.twig') }}


    <div class="form-group">{{ form_row(form.nom) }}</div>
    <div class="form-group">{{ form_row(form.prenom) }}</div>
    <div class="form-group">{{ form_row(form.email) }}</div>
    <div class="form-group">{{ form_row(form.pwd) }}</div>
    <div class="form-group">{{ form_row(form.poste) }}</div>
    <div class="form-group">{{ form_row(form.salaire) }}</div>
    <div class="form-group">{{ form_row(form.role) }}</div>
    {{ form_end(form) }}
    </div>

{% endblock %}
